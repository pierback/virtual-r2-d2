{
    "dom": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es6": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-unused-vars": 0,
        "no-undef": 2,
        "no-process-env": "warn",
        "no-console": 0,
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-unreachable": "warn",
        "no-var": "error",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "no-extra-semi": "warn",
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        },
        "sourceType": "module"
    }
}